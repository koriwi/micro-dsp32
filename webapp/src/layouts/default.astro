---
import "../style.css";
---

<html class="size-full flex min-h-0">
  <head>
    <title>Micro DSP</title>
    <link
      rel="icon"
      type="image/svg+xml"
      href={`data:image/svg+xml;base64,`}
    />
    <style>
      html {
        color-scheme: dark;
        background: black;
      }

      select option {
        background: black;
      }
    </style>
  </head>
  <body class="size-full flex flex-col min-h-0 overflow-hidden">
    <slot />
  </body>
</html>
